import java.util.Scanner;

public class javabasics{
    public static int fact=1;
    public static void printingFactorialOfnUsingRecursion(int n){
        
        if(n==1 || n==0){
            System.out.println(fact);
            return ;
        }
        // System.out.println(fact);
        fact=fact*n;
        printingFactorialOfnUsingRecursion(n-1);
        
        
    }
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        printingFactorialOfnUsingRecursion(n);   
    }
}
